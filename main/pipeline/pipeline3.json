{
	"name": "pipeline3",
	"properties": {
		"activities": [
			{
				"name": "WebFirst",
				"type": "WebActivity",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"url": "https://graph.microsoft.com/v1.0",
					"method": "GET",
					"headers": {
						"calender": "/me/calendars"
					},
					"linkedServices": [
						{
							"referenceName": "AzureDataLakeStore1",
							"type": "LinkedServiceReference"
						}
					],
					"datasets": [
						{
							"referenceName": "AzureDataLakeStoreFile1",
							"type": "DatasetReference"
						}
					]
				}
			}
		],
		"parameters": {
			"time": {
				"type": "String",
				"defaultValue": "@{activity('WebFirst').output.token} "
			}
		}
	}
}